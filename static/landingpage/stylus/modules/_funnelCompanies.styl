lesscss-percentage(n)
  (n * 100)%
.m-funnelCompanies
  padding 60px 10%
  width 80%
  .m-funnelCompanies__title
    font-weight 400
    padding 0 0 30px 0
  .m-funnelCompaniesContainer
    padding 0 0 30px 0
    width 100%
    text-align center
    .m-funnelCompanies__image
      height 75px
      margin 0 2%
      display inline-block
      vertical-align middle
    .starter_footer_button
      padding 9% 0 9% 0
    .starter_try_it_button
      text-transform none
      padding 1.69% 8% 1.69% 8%
      text-decoration none
      color white

.full_width {
    width: 100%;
    height: auto;
    max-height: 100%;
    max-width: 100%;
}
.logos_inline{
    display: flex;
    justify-content: space-between;
}

.div_logo{
    max-width: none;
    width: 90%;
}
.m-funnelCompanies .m-funnelCompaniesContainer .m-funnelCompanies__image.logo_height{
    height: auto;
    margin: 0;
}


@media screen and (max-width: max-mobile-screen)
  .m-funnelCompanies
    padding 120px 5%
    width 90%
    .m-funnelCompaniesContainer
      padding 0
      .m-funnelCompanies__image
        margin 0 auto 30px auto
        width 40%
        height auto
        float none
        display block

@media screen and (min-width: max-mobile-screen) and (max-width: 1025px)
  .m-funnelCompanies
    .m-funnelCompaniesContainer
      .m-funnelCompanies__image
        height 50px

@media screen and (min-width: 320px) and (max-width: 767px)
  .m-funnelCompanies
    .m-funnelCompaniesContainer
      .starter_try_it_button
       padding 4px
       font-size 8px
