lesscss-percentage(n)
  (n * 100)%
.m-mainVideoContainer
  position absolute
  z-index -100
  min-height 600px
  top 0
  right 0
  bottom 0
  left 0
  overflow-x hidden
  background #292125
  .m-mainVideo
    position absolute
    top 50%
    left 50%
    min-width 100%
    min-height 100%
    width auto
    height auto
    z-index -100
    transform translateX(-50%) translateY(-50%)
  .m-mainVideo__image
    width 100%
    height 100%
    background-image url("/static/landingpage/vid/landing_intro_poster.png")
    background-size cover
    background-position-x center
